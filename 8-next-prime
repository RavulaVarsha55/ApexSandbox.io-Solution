public Integer nextPrime(Integer num) {
    //code here
    Integer count=0;
    Boolean isPrime=false;

    while(!isPrime){
        Integer thisnum=num+1;
    for(Integer i=2;i<=thisnum-1;i++){
        if (Math.mod(thisnum,i)==0){
            isPrime=true;
            break;
        }
    }
    if(isPrime)
    {
        return thisnum;
    }
return;
    }
}