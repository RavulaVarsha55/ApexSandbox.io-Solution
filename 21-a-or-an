public String aOrAn(String word) {
    //code here
    list <String> vowelList= new list <String>{'a','e','i','o','u'};
    for(String si:vowelList){
    if(word.startsWith(si)){
        //word='an '+word;
            return 'an '+word;

    }
    else{
        //word='a '+word;
            return 'a '+word;

    }
}
            return word;

}