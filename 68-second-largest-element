public Integer secondLargest(List<Integer> numbers) {
    //code here
    numbers.sort();
    Integer max=numbers[numbers.size()-1];
    Integer maxSecond=numbers[numbers.size()-2];

    for(Integer i=numbers.size()-2;i>=0;i--){
        if(maxSecond<numbers[i]){
            maxSecond=numbers[i];
            return maxSecond;
        }

    }
}